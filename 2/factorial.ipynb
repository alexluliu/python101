   "source": [
    "def factorial(n):\n",
    "    if n < 0:\n",
    "        print 'input negative value is invalid'\n",
    "        return None\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    product = 1\n",
    "    for i in range(1, n+1):\n",
    "        product = product * i\n",
    "    return product"
   ]
